{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "first-webpart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/firstWebpart/FirstWebpartWebPart.js",
          "manifest": "./src/webparts/firstWebpart/FirstWebpartWebPart.manifest.json"
        }
      ]
    },
    "fluent-ui-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/fluentUi/FluentUiWebPart.js",
          "manifest": "./src/webparts/fluentUi/FluentUiWebPart.manifest.json"
        }
      ]
    },
    "simplefomr-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/simplefomr/SimplefomrWebPart.js",
          "manifest": "./src/webparts/simplefomr/SimplefomrWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "FirstWebpartWebPartStrings": "lib/webparts/firstWebpart/loc/{locale}.js",
    "FluentUiWebPartStrings": "lib/webparts/fluentUi/loc/{locale}.js",
    "SimplefomrWebPartStrings": "lib/webparts/simplefomr/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}